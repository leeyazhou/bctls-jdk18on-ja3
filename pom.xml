				<version>1.3.2</version>
			</dependency>
			<dependency>
				<groupId>com.squareup.okhttp3</groupId>
				<artifactId>okhttp-bom</artifactId>
				<version>4.12.0</version>
				<scope>import</scope>
				<type>pom</type>
