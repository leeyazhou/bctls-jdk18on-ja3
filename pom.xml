	<modelVersion>4.0.0</modelVersion>
	<parent>
		<groupId>com.bytesgo</groupId>
		<artifactId>bytesgo-parent</artifactId>
		<version>6</version>
	</parent>
	<groupId>com.github.leeyazhou</groupId>
	<artifactId>bctls-jdk18on-ja3-parent</artifactId>
	<version>1.78.1.0-SNAPSHOT</version>
	<packaging>pom</packaging>
	<modules>
		<module>bctls-jdk18on-ja3</module>
	</modules>

	<properties>
		<maven.compiler.source>8</maven.compiler.source>
		<maven.compiler.target>8</maven.compiler.target>
		<project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
		<maven.test.skip>false</maven.test.skip>
		<bouncycastle.version>1.78.1</bouncycastle.version>
		<kotlin.version>1.9.25</kotlin.version>
		<httpclient5.version>5.4.1</httpclient5.version>
	</properties>
	<name>impersonator</name>
	<description>Spoof TLS/JA3/JA4 and HTTP/2 fingerprints in Java</description>
	<url>https://github.com/zhkl0228/impersonator</url>
	<licenses>
		<license>
			<name>The Apache Software License, Version 2.0</name>
			<url>https://www.apache.org/licenses/LICENSE-2.0.txt</url>
		</license>
	</licenses>
	<developers>
		<developer>
			<name>zhkl0228</name>
			<email>zhkl0228@gmail.com</email>
		</developer>
	</developers>
	<scm>
		<connection>scm:git:git@github.com:zhkl0228/impersonator.git</connection>
		<developerConnection>scm:git:git@github.com:zhkl0228/impersonator.git</developerConnection>
		<url>git@github.com:zhkl0228/impersonator.git</url>
	</scm>
	<build>
		<extensions>
			<extension>
				<groupId>kr.motd.maven</groupId>
				<artifactId>os-maven-plugin</artifactId>
				<version>1.7.1</version>
			</extension>
		</extensions>
	</build>
	<dependencies>
